# DF options

config GRAPHICS
	bool
	prompt "Include DFGraphics"
	default y
	help
	  Select this to download all the DFGraphics tilesets.

config GRAPHICS_VERSION
	string
	prompt "Github revision of DFGraphics"
	default "master"
	depends on GRAPHICS
	help
	  DFGraphics revision from https://github.com/DFgraphics.

